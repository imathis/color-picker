  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vite": "^7.0.0",
    "vitest": "^3.1.2"
  },
  "resolutions": {
    "esbuild": "0.25.3"
  },
  "packageManager": "yarn@4.9.1"
